/var/log/nginx/error.log
/var/log/nginx/revsw_nginx_access_json*.log  {
        daily
        missingok
        rotate 4
        compress
        delaycompress
        notifempty
        create 0640 www-data adm
        sharedscripts
        postrotate
                [ ! -f /var/run/nginx.pid ] || kill -USR1 `cat /var/run/nginx.pid`
        endscript
}
